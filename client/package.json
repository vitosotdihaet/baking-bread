{
    "name": "baking-bread-main",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "next dev",
        "dev:analyze": "ANALYZE=true npm run dev",
        "lint": "next lint",
        "build": "next build",
        "build:analyze": "ANALYZE=true npm run build",
        "start": "next start",
        "start:prod": "npm run build && npm run start",
        "lint:ts": "eslint \"**/*.{ts,tsx}\"",
        "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
        "lint:scss": "npx stylelint \"**/*.scss\"",
        "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
        "test:unit": "jest --config ./config/jest/jest.config.ts",
        "test:ui": "npx loki test",
        "test:ui:ok": "npx loki approve",
        "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
        "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
        "test:ui:json": "node scripts/loki/generate-visual-json-report.js",
        "test:ui:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
        "storybook": "storybook dev -p 6006 -c ./config/storybook",
        "build-storybook": "storybook build -c ./config/storybook"
    },
    "dependencies": {
        "next": "^13.2.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.21.0",
        "@next/bundle-analyzer": "^13.2.4",
        "@next/eslint-plugin-next": "^13.2.4",
        "@storybook/addon-essentials": "^7.0.0-rc.5",
        "@storybook/addon-interactions": "^7.0.0-rc.5",
        "@storybook/addon-links": "^7.0.0-rc.5",
        "@storybook/addon-storyshots": "^6.5.16",
        "@storybook/blocks": "^7.0.0-rc.5",
        "@storybook/nextjs": "^7.0.0-rc.5",
        "@storybook/react": "^7.0.0-rc.5",
        "@storybook/testing-library": "^0.0.14-next.1",
        "@svgr/webpack": "^6.5.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.5.0",
        "@types/react": "18.0.28",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "babel-plugin-inline-react-svg": "^2.0.2",
        "eslint": "^8.36.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-next": "^13.2.4",
        "eslint-plugin-i18next": "^6.0.0-7",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-next": "^0.0.0",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.11",
        "file-loader": "^6.2.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "next-compose-plugins": "^2.2.1",
        "next-react-svg": "^1.2.0",
        "puppeteer": "^19.7.5",
        "reg-cli": "^0.17.7",
        "regenerator-runtime": "^0.13.11",
        "sass": "^1.59.3",
        "storybook": "^7.0.0-rc.5",
        "storycap": "^4.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.2",
        "webpack": "^5.76.2"
    }
}
